{
  "name": "datastar-ssegen",
  "version": "0.1.3",
  "description": "Datastar Server-Sent Event generator",
  "author": "John Cudd",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "concurrently -c \"red,green,blue,cyan\" -n \"node,express,hyper-express,bun\" \"npm run node\" \"npm run express\" \"npm run hyper-express\" \"npm run bun\"",
    "node": "nodemon examples/node.example.js",
    "express": "nodemon examples/express.example.js",
    "hyper-express": "nodemon examples/hyper-express.example.js",
    "bun": "bun examples/bun.example.js",
    "check-types": "tsc --project ./jsconfig.json"
  },
  "keywords": [
    "datastar",
    "hypermedia",
    "sse"
  ],
  "license": "mit",
  "homepage": "https://github.com/jmcudd/datastar-ssegen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcudd/datastar-ssegen.git"
  },
  "bugs": {
    "url": "https://github.com/jmcudd/datastar-ssegen/issues"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "express": "^4.21.2",
    "hyper-express": "^6.17.3",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  }
}
